<!doctype html>
	<html>

	<head>
		
	</head>

	<body>

		<script type="text/javascript">
			function printtime()
			{
				var now=new Date();
				var hours=now.getHours();
				var mins=now.getMinutes();
				var sec=now.getSeconds();
				document.write(hours+":"+mins+":"+sec+"</br>");
			}
			setInterval("printtime()",1000);

		</script>
	</body>
	</html>
